                        <TextBlock Text="Code" Foreground="Black" FontSize="16" Margin="0,16,0,0" HorizontalAlignment="Center" FontWeight="DemiBold"> <TextBlock.Effect><BlurEffect KernelType="Box" Radius="2" /> </TextBlock.Effect> </TextBlock>
                        <TextBlock Text="Code" Foreground="Black" FontSize="16" Margin="0,16,0,0" HorizontalAlignment="Center" FontWeight="DemiBold"> <TextBlock.Effect><BlurEffect KernelType="Box" Radius="2" /> </TextBlock.Effect> </TextBlock>
