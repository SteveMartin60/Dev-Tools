    <!--....................................................................-->
    <!--#region Begin Region ButtonBasicBorder                              -->
    <!--....................................................................-->
    <Style x:Key="ButtonBasicBorder" TargetType="{x:Type Button}">
        <Setter Property="Template" x:Name="TestProperty">
            <!--............................................................-->
            <Setter.Value>
                <!--........................................................-->
                <ControlTemplate TargetType="{x:Type Button}">
                    <!--....................................................-->
                    <Border x:Name="Border" CornerRadius="5" Background="Transparent" Width="120" Height="120" BorderBrush="LightSteelBlue" BorderThickness="2" Margin="2,2,2,2" >
                    </Border>
                    <!--....................................................-->
                    <ControlTemplate.Triggers>
                        <!--................................................-->
                        <EventTrigger RoutedEvent="Loaded">
                            <!--............................................-->
                            <BeginStoryboard>
                                <Storyboard>
                                    <DoubleAnimation Storyboard.TargetName="Home"  Storyboard.TargetProperty="(Label.Opacity)" From="0.0" To="1.0" Duration="0:0:0.2"  />
                                </Storyboard>
                            </BeginStoryboard>
                            <!--............................................-->
                        </EventTrigger>
                        <!--................................................-->
                    </ControlTemplate.Triggers>
                    <!--....................................................-->
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--....................................................................-->
    <!--#rendegion Begin Region ButtonBasicBorder                           -->
    <!--....................................................................-->
    <Style x:Key="ButtonBase2"  TargetType="{x:Type Button}" BasedOn="{StaticResource ButtonBasicBorder}">
    </Style>
    <!--....................................................................-->
    <!--#endregion End Region ButtonBasicBorder                              -->
    <!--....................................................................-->
    <!--#region Begin Region ButtonBaseRound                                -->
    <!--....................................................................-->
    <Style TargetType="Button" x:Key="ButtonBaseRound">
        <Style.Resources>
            <Style TargetType="Border" x:Name="TestStyle">
                <Setter Property="CornerRadius" Value="5" />
            </Style>
        </Style.Resources>
        <Setter Property="Width"           Value="72"            />
        <Setter Property="BorderBrush"     Value="SteelBlue"   />
        <Setter Property="Background"      Value="Transparent" />
        <Setter Property="Foreground"      Value="White"       />
        <Setter Property="FontSize"        Value="26"            />
        <Setter Property="Margin"          Value="2,2,4,3"       />
        <Setter Property="Padding"         Value="-2,-4,0,0"     />
        <Setter Property="BorderThickness" Value="2,2,2,2"             />
        
    </Style>
    <!--....................................................................-->
    <!--#endregion End Region ButtonBaseRound                               -->
    <!--....................................................................-->
    <!--#region Begin Region ButtonRoundLarge                               -->
    <!--....................................................................-->
    <Style x:Key="ButtonRoundLarge"  TargetType="{x:Type Button}" BasedOn="{StaticResource ButtonBaseRound}">
        <Setter Property="Width"       Value="Auto"/>
        <Setter Property="BorderBrush" Value="SteelBlue"/>
        <Setter Property="Foreground"  Value="DodgerBlue"/>
        <Setter Property="Background"  Value="Transparent"/>
        <Setter Property="FontSize"    Value="20"/>
        <Setter Property="Margin"      Value="1"/>
        <Setter Property="Padding"      Value="1,-3,1,1"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
    </Style>
    <!--....................................................................-->
    <!--#endregion End Region ButtonRoundLarge                              -->
    <!--....................................................................-->
    <!--#region Begin Region ButtonRoundSmall                               -->
    <!--....................................................................-->
    <Style x:Key="ButtonRoundSmall"  TargetType="{x:Type Button}" BasedOn="{StaticResource ButtonBaseRound}">
        <Setter Property="Width"       Value="40"/>
        <Setter Property="Height"      Value="40"/>
        <Setter Property="BorderBrush" Value="LightSteelBlue"/>
        <Setter Property="Foreground"  Value="White"/>
        <Setter Property="Background"  Value="Transparent"/>
        <Setter Property="FontSize"    Value="15"/>
        <Setter Property="Margin"      Value="1"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
    </Style>
    <!--....................................................................-->
    <!--#endregion End Region ButtonRoundSmall                              -->
    <!--....................................................................-->
    <!--#region Begin Region ButtonRoundBlue                                -->
    <!--....................................................................-->
    <Style x:Key="ButtonRoundBlue"  TargetType="{x:Type Button}" BasedOn="{StaticResource ButtonRoundLarge}">
        <Setter Property="Background"  Value="#441E90FF"/>
    </Style>
    <!--....................................................................-->
    <!--#endregion End Region ButtonRoundLarge                              -->
    <!--....................................................................-->
    <!--#region Begin Region ButtonCrosshairs                               -->
    <!--....................................................................-->
    <Style x:Key="ButtonStyleCrosshairs" TargetType="{x:Type Button}">
        <Setter Property="Foreground"                 Value="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}"/>
        <Setter Property="Background"                 Value="Transparent"/>
        <Setter Property="Padding"                    Value="5"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment"   Value="Center"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <StackPanel>
                        <Image Source="/images/crosshairs-small.png"/>
                    </StackPanel>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--....................................................................-->
    <!--#endregion End Region ButtonCrosshairs                              -->
    <!--....................................................................-->
    <!--#region Begin Region ButtonHandStop                                 -->
    <!--....................................................................-->
    <Style TargetType="{x:Type Button}" x:Key="ButtonHandStop">
        <!--................................................................-->
        <Setter Property="Template">
            <!--............................................................-->
            <Setter.Value>
                <!--........................................................-->
                <ControlTemplate TargetType="{x:Type Button}">
                    <!--....................................................-->
                    <Viewbox>
                        <!--................................................-->
                        <Border x:Name="Border" CornerRadius="5" Background="Transparent" Width="120" Height="120" BorderBrush="LightSteelBlue" BorderThickness="2" Margin="2,2,2,2" >
                            <!--............................................-->
                            <Image x:Name="StopSign"  Margin="2,2,2,2" IsHitTestVisible="False" Source="/images/stop-sign-hand.png" />
                            <!--............................................-->
                        </Border>
                        <!--................................................-->
                    </Viewbox>
                    <!--....................................................-->
                    <ControlTemplate.Triggers>
                        <!--................................................-->
                        <EventTrigger RoutedEvent="Loaded">
                            <!--............................................-->
                            <BeginStoryboard>
                                <Storyboard>
                                    <DoubleAnimation Storyboard.TargetName="StopSign"  Storyboard.TargetProperty="(Label.Opacity)" From="0.0" To="1.0" Duration="0:0:0.2"  />
                                </Storyboard>
                            </BeginStoryboard>
                            <!--............................................-->
                        </EventTrigger>
                        <!--................................................-->
                    </ControlTemplate.Triggers>
                    <!--....................................................-->
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--....................................................................-->
    <!--#endregion End Region ButtonHandStop                                -->
