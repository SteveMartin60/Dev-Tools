<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    >
    <!--....................................................................-->
    <!--#region Begin Region StackPanelStyleBasic                               -->
    <!--....................................................................-->
    <Style x:Key="StackPanelStyleBasic" TargetType="{x:Type Label}">
        <Setter Property="ItemsControl.Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Border}">
                    <StackPanel>
                        <Image Source="/images/crosshairs-small.png"/>
                    </StackPanel>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--....................................................................-->
    <!--#endregion End Region StackPanelStyleBasic                              -->
    <!--....................................................................-->
    <!--#region Begin Region ButtonReturnHome                              -->
    <!--....................................................................-->
    <Style TargetType="Border" x:Key="GlobalStackPanelStyle">
    </Style>
    <!--....................................................................-->
    <!--#endregion End Region ButtonReturnHome                             -->
    <!--....................................................................-->

</ResourceDictionary>
    